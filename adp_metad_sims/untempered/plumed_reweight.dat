UNITS ENERGY=kcal/mol LENGTH=A TIME=ps

# set up two variables for Phi and Psi dihedral angles 
phi: TORSION ATOMS=5,7,9,15
psi: TORSION ATOMS=7,9,15,17

metad: METAD ARG=phi,psi PACE=1000000000 HEIGHT=0.0 SIGMA=0.3,0.3 FILE=hills_copy.dat GRID_MIN=-pi,-pi GRID_MAX=pi,pi CALC_RCT RESTART=YES

PRINT ARG=phi,psi,metad.bias STRIDE=1 FILE=colvar_reweight
